{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Evaluation (from debug_config.yaml)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/evaluation/evaluate.py",
            "args": [
                "--config_file",
                "debug_config.yaml"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/evaluation",
            "justMyCode": false,
            "env": {}
        },
        {
            "name": "Debug Evaluation (custom args)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/evaluation/evaluate.py",
            "args": [
                "--dataset", "longbench-e",
                "--data_dir", "hotpotqa_e",
                "--press_name", "snapkv",
                "--compression_ratio", "0.5",
                "--model", "/NV1/ykw/models/Meta-Llama-3.1-8B-Instruct",
                "--fraction", "0.01"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/evaluation",
            "justMyCode": false,
            "env": {}
        }
    ]
}
